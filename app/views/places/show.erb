<h3><%=@place.name%></h3>
<% @place.attributes.each do |key, value| %>
  <p><%= "#{key}: #{value}" %></p>
<% end %>






<form action="/places/<%=@place.id%>/delete" method="post">
  <input type="hidden" id="hidden" name="_method" value="delete">
  <input type="submit" value="Delete Place">
</form>